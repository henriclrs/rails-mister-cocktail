<div class="container text-center">
  <div class="form-group">
    <%= simple_form_for @dose, :url => {:action => "create"} do |f| %>
      <%= f.input :description %>
      <% f.association :cocktail %>
      <%= f.association :ingredient %>
      <%= f.submit "Validate", class: "btn btn-primary" %>
      <%= link_to '<i class="fas fa-undo-alt"></i> Back'.html_safe, cocktails_path, class: "btn btn-primary"%>
    <% end %>
  </div>
</div>
